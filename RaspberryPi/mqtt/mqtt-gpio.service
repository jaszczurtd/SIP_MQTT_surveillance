[Unit]
Description=MQTT GPIO Controller
After=network-online.target wg-quick@wg0.service
Requires=network-online.target wg-quick@wg0.service


[Service]
ExecStart=/home/pi/Documents/SIP_MQTT_surveillance/RaspberryPi/mqtt/mqtt_gpio
WorkingDirectory=/home/pi/Documents/SIP_MQTT_surveillance/RaspberryPi/mqtt/
Restart=always
RestartSec=5
User=pi
Group=gpio

[Install]
WantedBy=multi-user.target
